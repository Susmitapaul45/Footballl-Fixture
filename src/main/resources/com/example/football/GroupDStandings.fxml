<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="500.0" prefWidth="900.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.football.GroupDStandings">
    <children>
        <TableView fx:id="table" layoutX="172.0" layoutY="53.0" prefHeight="170.0" prefWidth="592.0">
            <columns>
                <TableColumn fx:id="name" prefWidth="104.00000303983688" text="Team" />
                <TableColumn fx:id="matchPlayed" prefWidth="84.00006103515625" text="Match Played" />
                <TableColumn fx:id="won" prefWidth="97.60000610351562" text="Won" />
                <TableColumn fx:id="lose" prefWidth="107.20001220703125" text="Lose" />
                <TableColumn fx:id="draw" prefWidth="102.39996337890625" text="Draw" />
                <TableColumn fx:id="points" prefWidth="102.4000244140625" text="Points" />
            </columns>
        </TableView>
        <Button fx:id="Home" layoutX="44.0" layoutY="37.0" mnemonicParsing="false" onAction="#onHomeButtonCLick" text="Home">
            <font>
                <Font name="System Bold Italic" size="15.0" />
            </font>
        </Button>
        <Button fx:id="Logout" layoutX="774.0" layoutY="420.0" mnemonicParsing="false" onAction="#onLogoutButtonCLick" style="-fx-background-color: #F80722;" text="Logout">
            <font>
                <Font name="System Bold Italic" size="15.0" />
            </font>
        </Button>
      <Button fx:id="Back" layoutX="81.0" layoutY="420.0" mnemonicParsing="false" onAction="#onBackButtonCLick" style="-fx-background-color: #424EF6;" text="Back">
         <font>
            <Font name="System Bold Italic" size="15.0" />
         </font>
      </Button>
    </children>
</AnchorPane>
