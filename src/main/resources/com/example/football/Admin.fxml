<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="500.0" prefWidth="900.0" style="-fx-background-color: #F3B0DF;" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.football.Admin">
   <children>
      <Button  layoutX="42.0" layoutY="38.0" mnemonicParsing="false" onAction="#onHomeButtonCLick" text="Home">
         <font>
            <Font name="System Bold Italic" size="15.0" />
         </font>
      </Button>
      <Button layoutX="55.0" layoutY="438.0" mnemonicParsing="false" onAction="#OnBackButtonClick" style="-fx-background-color: #6B7DD6;" text="Back">
         <font>
            <Font name="System Bold Italic" size="15.0" />
         </font>
      </Button>
      <Button layoutX="798.0" layoutY="438.0" mnemonicParsing="false" onAction="#onLogoutButtonCLick" style="-fx-background-color: #F80C28;" text="Logout">
         <font>
            <Font name="System Bold Italic" size="15.0" />
         </font>
      </Button>
      <Button layoutX="155.0" layoutY="160.0" mnemonicParsing="false" onAction="#onScheduleNewMatchButtonCLick" prefHeight="125.0" prefWidth="225.0" style="-fx-background-color: #B0F3F3;" text="Schedule new match">
         <font>
            <Font name="System Bold Italic" size="20.0" />
         </font>
      </Button>
      <Button layoutX="545.0" layoutY="160.0" minHeight="-Infinity" mnemonicParsing="false" onAction="#onUpdateFixtureButtonClick" prefHeight="125.0" prefWidth="225.0" style="-fx-background-color: #B0F3F3;" text="Update Fixture">
         <font>
            <Font name="System Bold Italic" size="20.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
